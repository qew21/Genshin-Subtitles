<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <sys:String x:Key="App_Title" xmlns:sys="clr-namespace:System;assembly=mscorlib">言語パック管理</sys:String>
    <sys:String x:Key="UI_Language" xmlns:sys="clr-namespace:System;assembly=mscorlib">UI 言語：</sys:String>
    <sys:String x:Key="Select_Game" xmlns:sys="clr-namespace:System;assembly=mscorlib">ゲーム：</sys:String>
    <sys:String x:Key="Recognize_Lang" xmlns:sys="clr-namespace:System;assembly=mscorlib">入力言語：</sys:String>
    <sys:String x:Key="Output_Lang" xmlns:sys="clr-namespace:System;assembly=mscorlib">出力言語：</sys:String>

    <sys:String x:Key="Game_Genshin" xmlns:sys="clr-namespace:System;assembly=mscorlib">原神</sys:String>
    <sys:String x:Key="Game_StarRail" xmlns:sys="clr-namespace:System;assembly=mscorlib">崩壊：スターレイル</sys:String>
    <sys:String x:Key="Game_zzz" xmlns:sys="clr-namespace:System;assembly=mscorlib">ゼンレスゾーンゼロ</sys:String>
    <sys:String x:Key="Game_Wuthering" xmlns:sys="clr-namespace:System;assembly=mscorlib">鳴潮</sys:String>


    <sys:String x:Key="Lang_ZH_Simplified" xmlns:sys="clr-namespace:System;assembly=mscorlib">簡体字中国語</sys:String>
    <sys:String x:Key="Lang_ZH_Traditional" xmlns:sys="clr-namespace:System;assembly=mscorlib">繁体字中国語</sys:String>

    <sys:String x:Key="Local_File_Info" xmlns:sys="clr-namespace:System;assembly=mscorlib">ローカルファイル情報</sys:String>
    <sys:String x:Key="Repo_Info" xmlns:sys="clr-namespace:System;assembly=mscorlib">リポジトリ情報</sys:String>
    <sys:String x:Key="Repo_Last_Modified" xmlns:sys="clr-namespace:System;assembly=mscorlib">最終更新：</sys:String>
    <sys:String x:Key="Download_Input_Lang" xmlns:sys="clr-namespace:System;assembly=mscorlib">入力言語をダウンロード：</sys:String>
    <sys:String x:Key="Download_Output_Lang" xmlns:sys="clr-namespace:System;assembly=mscorlib">出力言語をダウンロード：</sys:String>

    <sys:String x:Key="Status_Loading" xmlns:sys="clr-namespace:System;assembly=mscorlib">データ読み込み中……</sys:String>

    <sys:String x:Key="Btn_Sync" xmlns:sys="clr-namespace:System;assembly=mscorlib">同期</sys:String>
    <sys:String x:Key="Btn_Download_Input" xmlns:sys="clr-namespace:System;assembly=mscorlib">入力を取得</sys:String>
    <sys:String x:Key="Btn_Download_Output" xmlns:sys="clr-namespace:System;assembly=mscorlib">出力を取得</sys:String>
    <sys:String x:Key="Btn_Restart" xmlns:sys="clr-namespace:System;assembly=mscorlib">再起動</sys:String>
    <sys:String x:Key="Btn_Language" xmlns:sys="clr-namespace:System;assembly=mscorlib">OCR</sys:String>
    <sys:String x:Key="Btn_Convert" xmlns:sys="clr-namespace:System;assembly=mscorlib">SRT</sys:String>
    <sys:String x:Key="Btn_Region" xmlns:sys="clr-namespace:System;assembly=mscorlib">領域</sys:String>

    <sys:String x:Key="Help_Sync" xmlns:sys="clr-namespace:System;assembly=mscorlib">リモートリポジトリと同期して、最新の言語パックとメタデータを取得します。</sys:String>
    <sys:String x:Key="Help_Download_Input" xmlns:sys="clr-namespace:System;assembly=mscorlib">現在の「認識言語」の言語パックまたは語彙リソースをダウンロードします。</sys:String>
    <sys:String x:Key="Help_Download_Output" xmlns:sys="clr-namespace:System;assembly=mscorlib">現在の「出力言語」の翻訳/置換リソースをダウンロードします。</sys:String>
    <sys:String x:Key="Help_Restart" xmlns:sys="clr-namespace:System;assembly=mscorlib">最新の設定または言語リソースを適用するためにアプリケーションを再起動します。</sys:String>
    <sys:String x:Key="Help_Language" xmlns:sys="clr-namespace:System;assembly=mscorlib">OCR 認識機能をテストし、既存のスクリーンショットを使用します。</sys:String>
    <sys:String x:Key="Help_Convert" xmlns:sys="clr-namespace:System;assembly=mscorlib">SRT 字幕ファイルの変換に使用します。「srt」フォルダ内に配置してください。</sys:String>
    <sys:String x:Key="Help_Region" xmlns:sys="clr-namespace:System;assembly=mscorlib">現在のすべてのスクリーンの認識結果をテストし、ログに保存します。</sys:String>
    <sys:String x:Key="Help_Folder" xmlns:sys="clr-namespace:System;assembly=mscorlib">プログラムフォルダを開く</sys:String>
</ResourceDictionary>
